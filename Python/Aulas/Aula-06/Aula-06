n1 = int(input("Digite um valor: "))
n2 = int(input("Digite outro valor: "))
s = n1 + n2
#print("O valor das somas entre",n1, "e", n2, "e igual a",s)
print("A sona entre {} e {} vale {}" .format(n1, n2, s))



n1 = int(input("Digite um valor: "))
n2 = int(input("Digite outro valor: "))
s = n1 + n2
print("O valor das somas e igual a {}".format(s))